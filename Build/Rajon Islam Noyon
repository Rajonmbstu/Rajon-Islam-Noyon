#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin >>n;
    vector<int>v(n);
    for(int i=0;i<n;i++)
    {
        cin>>v[i];
    }
    string s=to_string(v[n-1]);
    if(s[s.size()-1]=='0')
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;



    return 0;
}
